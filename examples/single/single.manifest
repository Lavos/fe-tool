routes:
 - type: "html"
   template: "examples/templates/index.html.template"
   prefix: "WEBSITE"
   req_path: "/"
 - type: "javascript"
   manifest: "examples/js/example.manifest"
   source: "examples/js/src"
   req_path: "/app.min.js"
 - type: "sass"
   manifest: "examples/sass/example.manifest"
   source: "examples/sass/src"
   req_path: "/style.css"
 - type: "static"
   source: "examples/static"
   req_path: "/static/*"
